<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>ai.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>scale</object-name>
    <object-name>aircraft</object-name>
    <object-name>bouton</object-name>
    <object-name>slip</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>scale</object-name>
    <object-name>aircraft</object-name>
    <object-name>slip</object-name>
    <emission>
      <red>   0.30 </red>
      <green> 0.30 </green>
      <blue>  0.30 </blue>
    </emission>
  </animation>


  <animation>
    <type>material</type>
    <object-name>bouton</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.50 </green>
      <blue>  0.50 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>face</object-name>
    <object-name>slip</object-name>
    <property>/orientation/roll-deg</property>
    <factor>1.0</factor>
    <axis>
      <x>1.0</x>
    </axis>
  </animation>

  <animation>
    <type>select</type>
    <object-name>face</object-name>
    <object-name>scale</object-name>
    <object-name>aircraft</object-name>
    <object-name>slip</object-name>
    <condition>
        <greater-than>
            <property>systems/electrical/volts</property>
            <value>16</value>
        </greater-than>
    </condition>
  </animation>


  <animation>
    <type>textranslate</type>
    <object-name>face</object-name>
    <property>/orientation/pitch-deg</property>
    <factor>0.0057</factor>
    <offset></offset>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>

</PropertyList>








